<div id="course-summary">
<p>
  <b>Name:</b>
  <%=h @course.name %>
</p>
<p>
  <b>Par:</b>
  <%=h @course.par%>
</p>
<p>
  <b>Description:</b>
  <%=h @course.description %>
</p>
</div>

<div id="course-card">
  <%= render :partial => "course", :object => @course %>
</div>

<%= link_to 'Edit', edit_course_path(@course) %> |
<%= link_to 'Back', courses_path %>